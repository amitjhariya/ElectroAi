{
  "name": "ravanai",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "proxy": "http://localhost:8989",
  "scripts": {
    "start": "electron .",
    "server": "node index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "electron-dev": "concurrently \"npm run server\" \"npm run client\" ",
    "electron-build": "npm run build && npm run electron-build && electron-dev",
    "electron-start": "electron .",
    "electron-pack": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@xenova/transformers": "^2.14.2",
    "axios": "^1.6.5",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "d3-dsv": "^2.0.0",
    "duck-duck-scrape": "^2.2.5",
    "esm": "^3.2.25",
    "express": "^4.18.2",
    "faiss-node": "^0.5.1",
    "http-proxy-middleware": "^2.0.6",
    "langchain": "^0.1.11",
    "multer": "^1.4.5-lts.1",
    "node-llama-cpp": "^2.8.5",
    "pdf-parse": "^1.1.1"
  },
  "devDependencies": {
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1"
  },
  "build": {
    "appId": "electroai.app.id",
    "productName": "ElectroAi",
    "directories": {
      "output": "dist"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis",
      "icon": "logo.ico"
    },
    "nsis": {
      "installerIcon": "logo.ico",
      "installerHeaderIcon": "logo.ico",
      "deleteAppDataOnUninstall": true
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  }
}
